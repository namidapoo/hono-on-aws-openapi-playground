{
	"name": "hono-on-aws-openapi-playground",
	"version": "0.1.0",
	"bin": {
		"hono-on-aws-openapi-playground": "bin/hono-on-aws-openapi-playground.js"
	},
	"scripts": {
		"prepare": "lefthook install",
		"preinstall": "npx only-allow bun",
		"build": "tsc",
		"watch": "tsc -w",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest --coverage",
		"cdk": "cdk",
		"format": "bunx --bun biome format --write .",
		"lint": "bunx --bun biome lint .",
		"lint:fix": "bunx --bun biome lint --write .",
		"check": "bunx --bun biome check --write ."
	},
	"devDependencies": {
		"@biomejs/biome": "2.0.6",
		"@types/node": "22.7.9",
		"@vitest/ui": "^3.2.4",
		"aws-cdk": "2.1020.2",
		"lefthook": "^1.11.16",
		"ts-node": "^10.9.2",
		"typescript": "~5.6.3",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"aws-cdk-lib": "2.202.0",
		"constructs": "^10.0.0",
		"prettier": "^3.6.2"
	}
}
